{
  "name": "acceptance-test-example-api",
  "private": true,
  "scripts": {
    "start": "yarn ts-node -r tsconfig-paths/register ./src/Main.ts",
    "start:prod": "node dist/src/Main",
    "build": "yarn checks && yarn compile",
    "checks": "yarn lint && yarn test",
    "lint": "yarn eslint --max-warnings=0 \"./src/**/*.{ts,tsx}\"",
    "test": "yarn jest test --passWithNoTests --watchAll=false --config jest.config.js",
    "compile": "yarn clean && yarn ts-node -r tsconfig-paths/register scripts/Esbuild.ts && cp ../shared/src/graphql/schema/schema.graphql ./dist/src/schema.graphql",
    "clean": "rimraf dist",
    "test:e2e": "yarn jest test --passWithNoTests --watchAll=false --config jest.config.js"
  },
  "devDependencies": {
    "@apollo/server": "4.1.1",
    "@types/jest": "29.1.2",
    "@types/lodash": "4.14.188",
    "@types/node": "18.11.9",
    "@types/node-cron": "3.0.5",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "concurrently": "7.5.0",
    "eslint": "8.25.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.2",
    "eslint-plugin-no-relative-import-paths": "1.4.0",
    "graphql": "16.6.0",
    "jest": "29.2.0",
    "lodash": "4.17.21",
    "node-cron": "3.0.2",
    "nodemon": "2.0.20",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4",
    "uuid": "9.0.0"
  }
}
