    "@types/jest": "29.1.2",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "8.25.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.2",
    "eslint-plugin-no-relative-import-paths": "1.4.0",
    "jest": "29.2.0",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  }
